{
  "presets": ["env", "es2015", "stage-0", "react"],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs", "transform-class-properties"]
    },
    "development": {
      "plugins": ["transform-runtime", "react-docgen"]
    },
    "production": {
      "plugins": ["transform-runtime", "transform-react-remove-prop-types"]
    },
  },
}
